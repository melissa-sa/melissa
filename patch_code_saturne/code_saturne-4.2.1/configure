/* end confdefs.h.  */
#include <stdlib.h>
#include <CORBA.h>
int
main ()
{
CORBA::Double *a=new CORBA::Double(2.5);
  double c=2.5;
  double *b;
  b=(double *)a;

  if( (c==*b) && (sizeof(double)==sizeof(CORBA::Double)) ){
    delete a;
    exit(0);
  }
  else{
    delete a;
    exit(1);
  }
  ;
  return 0;
}
_ACEOF

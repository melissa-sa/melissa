  enableval=$enable_mpi_io;
    case "${enableval}" in
      yes) mpi_io=true ;;
      no)  mpi_io=false ;;
      *)   as_fn_error $? "bad value ${enableval} for --enable-mpi-io" "$LINENO" 5  ;;
    esac


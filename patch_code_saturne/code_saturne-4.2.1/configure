                     if test "x$cs_mpisupport" != "x" ; then
                       grep "Intel(R) MPI" "$cs_mpisupport" > /dev/null 2>&1
                       if test $? = 0 ; then
                         mpi_type=Intel_MPI

$as_echo "#define MPI_VENDOR_NAME \"Intel MPI\"" >>confdefs.h

                       fi
                     fi
                     unset cs_mpisupport
                     ;;

    conftest$ac_exeext conftest.$ac_ext
        fi
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: $cs_have_mpi" >&5
$as_echo "$cs_have_mpi" >&6; }
        ;;

      MSMPI)
        { $as_echo "$as_me:${as_lineno-$LINENO}: checking for MSMPI" >&5
$as_echo_n "checking for MSMPI... " >&6; }
        case $host_os in
          mingw32)
